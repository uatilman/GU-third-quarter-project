<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
        version="2.2">


    <managed-bean>
        <managed-bean-name>product</managed-bean-name>
        <managed-bean-class>ru.tilman.gb.ee.entity.Product</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <display-name>/admin/products/edit</display-name>
        <from-view-id>/admin/products/editproduct.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/admin/products/editproduct.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>